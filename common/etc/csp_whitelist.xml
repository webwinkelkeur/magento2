<?xml version="1.0"?>
<csp_whitelist xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Csp:etc/csp_whitelist.xsd">
    <policies>
        <policy id="script-src">
            <values>
                <value id="{{ LOWERCASE_PROJECT }}-sidebar" type="host">https://{{ DASHBOARD_DOMAIN }}/sidebar.js</value>
            </values>
        </policy>
        <policy id="font-src">
            <values>
                <value id="{{ LOWERCASE_PROJECT }}-sidebar-font" type="host">https://{{ DASHBOARD_DOMAIN }}fonts/webandbrand.woff</value>
            </values>
        </policy>
    </policies>
</csp_whitelist>
